<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="orderit.orderit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".OrderIt"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".insertintents.InsertDrinks"
            android:label="@string/title_activity_insert_drinks"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".insertintents.InsertBottles"
            android:label="@string/title_activity_insert_bottles"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".insertintents.InsertCustomers"
            android:label="@string/title_activity_insert_customers"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".insertintents.InsertOrder"
            android:label="@string/title_activity_insert_order"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>